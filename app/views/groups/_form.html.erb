  <%= form_with model:group do |f| %>
     <div>
       <div>
          <%= f.label :"グループ名"%>
          <%= f.text_field :name, class: 'form-control'%>
        </div>
        <div>
          <%= f.label :"紹介文" %>
          <%= f.text_area :introduction, class: 'form-control'%>
        </div>
        <div>
          <%= f.label :"グループ画像" %></br>
          <%= f.file_field :image, accept: "image/*"%>
        </div>
        <div>
          <%= f.hidden_field :user_ids, multiple: true %>
          <%= f.submit "Create Group",class: 'btn btn-success mt-3' %>
        </div>
      </div>
    <% end %>